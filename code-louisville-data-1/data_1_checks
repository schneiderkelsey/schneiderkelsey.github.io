# hello world statement
print("hello world")

# print list value
my_list = [
    "grasses/sedges/rushes", 
    "small trees and shrubs", 
    "ferns", 
    "wildflowers"
    ]

print(my_list[2])

# print dictionary value
my_dict = {
"small trees": "elderberry and river birch",
"wildflowers": "common milkweed and cardinal flower"
}
values = list(my_dict.values()) 

print(values[0])

# print tuple value
my_tuple = ("elderberry", "river birch", "common milkweed", "cardinal flower")

print(my_tuple[2])
